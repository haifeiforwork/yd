=\"\"\r\n                                            :src=\"item.thirdImg\"\r\n                                            @click=\"selectProduct(item,item.id)\"\r\n                                    />\r\n                                    <div class=\"shoucang\">\r\n                                        <van-icon class-prefix=\"icon\" class=\"iconfont\" name=\"shoucang\" :color=\"item.isCollect === 1 ?'#FFA500':'grey'\" @click=\"Collect(item.id)\"/>\r\n                                    </div>\r\n                                    <!-- secondId 为1, 代表是工业胶管-->\r\n                                    <div class=\"info-wraper\" v-if=\"item.secondId === 1\">\r\n                                        <div class=\"text\">\r\n                                            {{item.typeOne.productName}}\r\n                                        </div>\r\n                                        <div class=\"detail\">\r\n                                            <div class=\"desc\"> 内径: {{item.typeOne.hoseId }} Inch </div>\r\n                                            <div class=\"desc\"> 外径: {{item.typeOne.hoseOd }}mm</div>\r\n                                        </div>\r\n                                        <van-icon class-prefix=\"icon\" class=\"iconfont van-icon\" name=\"tianjia\" @click=\"addClick(item.id)\" :color=\"item.add ? 'grey' : '#FFA500'\"/>\r\n                                    </div>\r\n                                    <!-- secondId 为1, 代表是液压胶管-->\r\n                                    <div class=\"info-wraper\" v-else-if=\"item.secondId === 2\">\r\n                                        <div class=\"text\">\r\n                                            <b>{{item.typeTwo.partNumber}}</b>\r\n                                        </div>\r\n                                        <div class=\"detail\">\r\n                                            <div class=\"desc\"> 内径: {{item.typeTwo.hoseId }} Inch </div>\r\n                                            <div class=\"desc\"> 压力: {{item.typeTwo.wpPsi }}</div>\r\n                                        </div>\r\n                                        <van-icon class-prefix=\"icon\" class=\"iconfont van-icon\" name=\"tianjia\" @click=\"addClick(item.id)\" :color=\"item.add ? 'grey' : '#FFA500'\"/>\r\n                                    </div>\r\n\r\n                                    <div class=\"info-wraper\" v-else>\r\n                                        <div class=\"text\">\r\n                                            {{item.title}}\r\n                                        </div>\r\n                                        <div class=\"detail\">\r\n                                            <div class=\"desc desc-guidao\">{{item.describe }}</div>\r\n                                        </div>\r\n                                        <van-icon class-prefix=\"icon\" class=\"iconfont van-icon\" name=\"tianjia\" @click=\"addClick(item.id)\" :color=\"item.add ? 'grey' : '#FFA500'\"/>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n\r\n                        </minirefresh>\r\n                    </div>\r\n\r\n                </van-col>\r\n            </van-row>\r\n            <van-goods-action @click=\"disabled1=false,disabled2=false\">\r\n                <van-goods-action-button @click=\"showSelected\">\r\n                    <van-icon :info=\"result.length\" class-prefix=\"icon\" class=\"iconfont\" name=\"renwuqingdan\"/><span>已选择</span>\r\n                </van-goods-action-button>\r\n                <van-goods-action-button text=\"下一步 >\" @click=\"goNext\"/>\r\n            </van-goods-action>\r\n            <van-popup\r\n                v-model=\"show\"\r\n                round\r\n                position=\"bottom\"\r\n                :style=\"{ height: '50%',backgroundColor:'black',color:'white'}\"\r\n            >\r\n                <div class=\"selectButton\">\r\n                    <span class=\"selectAll\" @click=\"selectAll\"><u>全选</u></span>\r\n                    <span class=\"selectNotAll\" @click=\"selectNotAll\"><u>全不选</u></span>\r\n                </div>\r\n                <van-checkbox-group v-model=\"result\" checked-color=\"#07c160\">\r\n                <van-cell-group v-for=\"(item,index) in products\" :key=\"index\">\r\n                        <van-cell\r\n                            :key=\"item.id\"\r\n                            :style=\"{backgroundColor:'black',color:'white'}\"\r\n                            @click=\"toggle(index)\"\r\n                            :border=\"border\"\r\n                        >\r\n                            <div class=\"title\" v-if=\"item.secondId === 1\">\r\n                                {{item.typeOne.productName}}\r\n                                <span style=\"color: grey;\"> - 内径: {{item.typeOne.hoseId }} Inch - 外径: {{item.typeOne.hoseOd }}mm </span>\r\n                            </div>\r\n                            <div class=\"title\" v-else-if=\"item.secondId === 2\">\r\n                                {{item.typeTwo.partNumber}}\r\n                                <span style=\"color: grey;\"> - 内径: {{item.typeTwo.hoseId }} Inch - 压力: {{item.typeTwo.wpPsi }}</span>\r\n                            </div>\r\n                            <div class=\"title\" v-else>\r\n                                {{item.title}} / <span style=\"color: grey;\"> {{ item.describe }} </span>\r\n                            </div>\r\n                            <van-checkbox\r\n                                v-model=\"item.checked\"\r\n                                :name=\"item.thirdName\"\r\n                                ref=\"checkboxes\"\r\n                                :label-disabled=\"label\"\r\n                            >\r\n                                <van-icon\r\n                                    slot=\"icon\"\r\n                                    slot-scope=\"item\"\r\n                                    class-prefix=\"icon\"\r\n                                    class=\"iconfont icon-custom\"\r\n                                    :name=\"item.checked ? 'xuanzehou' : 'xuanzekuang'\"\r\n                                    color=\"white\"/>\r\n                            </van-checkbox>\r\n                        </van-cell>\r\n                    </van-cell-group>\r\n                </van-checkbox-group>\r\n                <van-goods-action>\r\n                <van-goods-action-button @click=\"showSelected\">\r\n                    <van-icon :info=\"result.length\" class-prefix=\"icon\" class=\"iconfont\" name=\"renwuqingdan\"/><span>已选择</span>\r\n                </van-goods-action-button>\r\n                <van-goods-action-button text=\"下一步 >\" @click=\"goNext\"/>\r\n            </van-goods-action>\r\n            </van-popup>\r\n        </div>\r\n</template>\r\n<script>\r\n    import '../../style/iconfont.css';\r\n    import { getFirstDirectory, getSecondDirectory, getThirdDirectory, getSearch, getAllInfos, getPageThrid, getAllcollect, changeCollect} from '@/api/directory'\r\n    import { BASE_API } from '@/constant';\r\n    import MiniRefresh from '../../components/minirefresh/minirefresh.vue';\r\n    import { mapGetters } from 'vuex';\r\n    import {TreeSelect,\r\n             Row,\r\n             Col,\r\n             Image,\r\n             GoodsAction,\r\n             GoodsActionIcon,\r\n             GoodsActionButton,\r\n             DropdownMenu,\r\n             DropdownItem,\r\n             Picker,\r\n             Icon,\r\n             Search,\r\n             Button,\r\n             Popup,\r\n             Checkbox,\r\n             CheckboxGroup,\r\n             Cell,\r\n             CellGroup\r\n            } from 'vant';\r\n\r\n    export default {\r\n        components: {\r\n            [Row.name]: Row,\r\n            [Col.name]: Col,\r\n            [TreeSelect.name]: TreeSelect,\r\n            [Image.name]: Image,\r\n            [GoodsAction.name]: GoodsAction,\r\n            [GoodsActionIcon.name]: GoodsActionIcon,\r\n            [GoodsActionButton.name]: GoodsActionButton,\r\n            [DropdownMenu.name]: DropdownMenu,\r\n            [DropdownItem.name]: DropdownItem,\r\n            [Picker.name]: Picker,\r\n            [Icon.name]: Icon,\r\n            [Search.name]: Search,\r\n            [Button.name]: Button,\r\n            [Popup.name]: Popup,\r\n            [Checkbox.name]: Checkbox,\r\n            [CheckboxGroup.name]: CheckboxGroup,\r\n            [Cell.name]: Cell,\r\n            [CellGroup.name]: CellGroup,\r\n            minirefresh: MiniRefresh\r\n        },\r\n        data() {\r\n            return {\r\n                text: \"\",\r\n                key: '',\r\n                thirdClass: false,\r\n                secondClass: true,\r\n                top: true,\r\n                screen: true,\r\n                data1: {text: '产品特性'},\r\n                data2: {text: '参数选择'},\r\n                default2:0,\r\n                default1:0,\r\n                itemheight: 44,\r\n                disabled1: false,\r\n                disabled2: false,\r\n                label: true,\r\n                show: false,\r\n                loading: false,\r\n                border:false,\r\n                checked: true,\r\n                list: [],\r\n                selectOptions2: [],\r\n                columns: [],\r\n                searchcontent: '',\r\n                search: false,\r\n                selectOptions1: [{id:\"collect\",firstName: '我的收藏',icon:\"shoucangjia\",style:'selectUl'},\r\n                                {id:\"search\",firstName: '快捷搜索',icon:\"sousuo\",style:'selectUl'},\r\n                ],\r\n                activeId: 1,\r\n                activeIndex: 0,\r\n                secondItem: [],\r\n                thirdItem: [],\r\n                type:Object,\r\n                pagination: {\r\n                    page: 1,\r\n                    pageSize: 10,\r\n                    total: 0\r\n                },\r\n                secondId: null,\r\n                options: {\r\n                    container: '#minirefresh',\r\n                    up: {\r\n                        isAuto: true,\r\n                        loadFull: {\r\n                            isEnable: true\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n        },\r\n\r\n        computed: {\r\n            // 使用对象展开运算符将 getter 混入 computed 对象中\r\n            ...mapGetters([\r\n                'products'\r\n            ]),\r\n            result:{\r\n                get(){\r\n                    return this.$store.state.app.result\r\n                },\r\n                set(val){\r\n                    this.$store.dispatch('app/setResult', val);\r\n                }\r\n            }\r\n        },\r\n\r\n        created() {\r\n            this.getData()\r\n        },\r\n\r\n\r\n        methods: {\r\n            onPullingDown() {\r\n                if (this.id === \"search\") {\r\n                        this.getSearchDirectoryData('down', true)\r\n                } else if (this.id === \"collect\") {\r\n                        this.getCollect('down', true)\r\n                } else if (this.data2.text == '参数选择') {\r\n                        this.getThirdDirectoryData('down', true);\r\n                    } else {\r\n                        this.getPage('down', true)\r\n                    }\r\n                },\r\n            onPullingUp() {\r\n                if (this.id === \"search\") {\r\n                        this.getSearchDirectoryData(\"up\")\r\n                    } else if (this.id === \"collect\") {\r\n                        this.getCollect(\"up\")\r\n                    } else if (this.data2.text == '参数选择') {\r\n                        this.getThirdDirectoryData('up');\r\n                    } else {\r\n                        this.getPage('up')\r\n                    }\r\n            },\r\n            getPage(pull, isReset) {\r\n                if (isReset) {\r\n                    this.pagination.page = 1;\r\n                }\r\n                getPageThrid({\r\n                    secondId: this.secondId,\r\n                    value: this.text,\r\n                    key: this.key,\r\n                    size: this.pagination.pageSize,\r\n                    page: this.pagination.page\r\n                }).then(res => {\r\n                    if (res.status === 200) {\r\n                        this.pagination.page = this.pagination.page + 1;\r\n                        this.pagination.size = res.data.pageSize;\r\n                        this.pagination.total = res.data.totalCount;\r\n                        if (res.data.data && res.data.data.length > 0) {\r\n                            let items = res.data.data;\r\n                            items.forEach(value => {\r\n                                return value.thirdImg = BASE_API + value.thirdImg\r\n                            })\r\n                            for (let i = 0; i < items.length; i++) {\r\n                                for (let j = 0; j < this.products.length; j++) {\r\n                                    if (items[i].id == this.products[j].id) {\r\n                                        items[i].add = \"yes\"\r\n                                    }\r\n                                }\r\n                            }\r\n                            if (pull === 'down') {\r\n                                this.thirdItem = items;\r\n                                this.$refs.minirefresh.endDownLoading(true);\r\n                            } else {\r\n                                this.thirdItem = this.thirdItem.concat(items);\r\n                                this.$refs.minirefresh.endUpLoading(this.thirdItem.length >= res.data.totalCount);\r\n                            }\r\n                        } else {\r\n                                this.thirdItem = [];\r\n                                this.$refs.minirefresh.endUpLoading(true);\r\n                            }\r\n                        }\r\n                })\r\n            },\r\n            getThirdDirectoryData(pull, isReset) {\r\n                if (isReset) {\r\n                    this.pagination.page = 1;\r\n                }\r\n                getThirdDirectory({\r\n                    secondId: this.secondId,\r\n                    pageSize: this.pagination.pageSize,\r\n                    page: this.pagination.page\r\n                }).then(res => {\r\n                    if (res.status === 200) {\r\n                        this.pagination.page = this.pagination.page + 1;\r\n                        this.pagination.pageSize = res.data.pageSize;\r\n                        this.pagination.total = res.data.total;\r\n                        if (res.data.list && res.data.list.length > 0) {\r\n                        let items = res.data.list;\r\n                        items.forEach( value => {\r\n                            return value.thirdImg = BASE_API + value.thirdImg\r\n                        })\r\n                        for (let i = 0; i < items.length; i++) {\r\n                            for (let j = 0; j < this.products.length; j++){\r\n                                if (items[i].id == this.products[j].id) {\r\n                                    items[i].add = \"yes\"\r\n                                }\r\n                            }\r\n                        }\r\n                        if (pull === 'down') {\r\n                            this.thirdItem = items;\r\n                            this.$refs.minirefresh.endDownLoading(true);\r\n                        } else {\r\n                                this.thirdItem = this.thirdItem.concat(items);\r\n                                this.$refs.minirefresh.endUpLoading(this.thirdItem.length >= res.data.total);\r\n                            } }else {\r\n                            this.thirdItem = [];\r\n                            this.$refs.minirefresh.endUpLoading(true);\r\n                        }\r\n                    }\r\n                })\r\n            },\r\n            getCollect(pull, isReset) {\r\n                if (isReset) {\r\n                    this.pagination.page = 1;\r\n                }\r\n                getAllcollect({\r\n                    userId:1\r\n                }).then(res => {\r\n                    if (res.status === 200) {\r\n                        this.pagination.page = this.pagination.page + 1;\r\n                        this.pagination.pageSize = res.data.pageSize;\r\n                        this.pagination.total = res.data.total;\r\n                        let items = res.data;\r\n                        items.forEach( value => {\r\n                            return value.thirdImg = BASE_API + value.thirdImg\r\n                        })\r\n                        for (let i = 0; i < items.length; i++) {\r\n                            for (let j = 0; j < this.products.length; j++){\r\n                                if (items[i].id == this.products[j].id) {\r\n                                    items[i].add = \"yes\"\r\n                                }\r\n                            }\r\n                        }\r\n                        if (pull === 'down') {\r\n                            this.thirdItem = items;\r\n                            this.$refs.minirefresh.endDownLoading(true);\r\n                        } else {\r\n                            this.thirdItem = this.thirdItem.concat(items);\r\n                            this.$refs.minirefresh.endUpLoading(this.thirdItem.length >= res.data.total);\r\n                        }\r\n                    }\r\n                })\r\n            },\r\n            getSearchDirectoryData(pull, isReset) {\r\n                if (isReset) {\r\n                    this.pagination.page = 1;\r\n                }\r\n                getSearch({\r\n                    thirdName:this.searchcontent,\r\n                    pageSize: this.pagination.pageSize,\r\n                    page: this.pagination.page\r\n                }).then(res => {\r\n                    if (res.status === 200) {\r\n                        this.pagination.page = this.pagination.page + 1;\r\n                        this.pagination.pageSize = res.data.pageSize;\r\n                        this.pagination.total = res.data.total;\r\n                        let items = res.data.list;\r\n                        items.forEach( value => {\r\n                            return value.thirdImg = BASE_API + value.thirdImg\r\n                        })\r\n                        for (let i = 0; i < items.length; i++) {\r\n                            for (let j = 0; j < this.products.length; j++){\r\n                                if (items[i].id == this.products[j].id) {\r\n                                    items[i].add = \"yes\"\r\n                                }\r\n                            }\r\n                        }\r\n                        if (pull === 'down') {\r\n                            this.thirdItem = items;\r\n                            this.$refs.minirefresh.endDownLoading(true);\r\n                        } else {\r\n                            this.thirdItem = this.thirdItem.concat(items);\r\n                            this.$refs.minirefresh.endUpLoading(this.thirdItem.length >= res.data.total);\r\n                        }\r\n                    }\r\n                })\r\n            },\r\n\r\n            getData() {\r\n                getFirstDirectory().then( res => {\r\n                    if (res.status === 200) {\r\n                        this.selectOptions2 = res.data\r\n                    }\r\n                    const selectedOptions2 = this.selectOptions2\r\n                    for (let i = 0; i < selectedOptions2.length; i++) {\r\n                     selectedOptions2[i].style = \"selectUl\"\r\n                     this.selectOptions2 = selectedOptions2\r\n                    }\r\n                })\r\n            },\r\n            onChange1(picker, value,index) {\r\n                this.data1= value\r\n                this.data2 = {text: '参数选择'}\r\n                this.default1 = parseInt(index)\r\n                this.default2 = 0\r\n                this.key = this.data1.key\r\n            },\r\n            onChange2(picker, value,index) {\r\n                this.thirdItem = []\r\n                this.pagination.page = 1\r\n                this.data2= value\r\n                this.default2 = parseInt(index)\r\n                this.text = this.data2.text\r\n                this.getPage()\r\n            },\r\n            selectClick1() {\r\n                this.disabled2 = false\r\n                const disabled1 = this.disabled1\r\n                this.disabled1 = !disabled1\r\n                this.columns = [{text:\"\"}].concat(this.type.productFeatures);\r\n                // console.log(this.columns)\r\n            },\r\n            selectClick2() {\r\n                if (this.data1.text === '产品特性') {\r\n                    return;\r\n                }\r\n                const type = this.type;\r\n                const disabled2 = this.disabled2;\r\n                this.disabled1 = false;\r\n                this.disabled2 = !disabled2;\r\n                this.columns = [{text:\"\"}].concat(type[this.data1.key]);\r\n\r\n            },\r\n            selectProduct(item,id) {\r\n                const params = {secondId:id}\r\n                getAllInfos(params).then(res=>{\r\n                    if (res.status === 200) {\r\n                        this.columns = res.data.productFeatures\r\n                        this.type = res.data\r\n                        this.default1 = 0\r\n                        this.default2 = 0\r\n                    }\r\n                })\r\n                this.secondId = id;\r\n                this.secondClass = false\r\n                this.thirdClass = true\r\n            },\r\n            onSearch() {\r\n                this.pagination.page = 1\r\n                this.thirdItem = []\r\n                this.getSearchDirectoryData()\r\n            },\r\n            addClick(id) {\r\n                const item = this.thirdItem\r\n                for (let i = 0; i < item.length; i++) {\r\n                    if (item[i].id == id && !item[i].add) {\r\n                        let temp = [].concat(this.products)\r\n                        temp.push(item[i])\r\n                        this.$store.dispatch('app/setProducts', temp);\r\n                        item[i].add = \"yes\"\r\n                        this.thirdItem = item\r\n                    }\r\n                }\r\n            },\r\n            Collect(id) {\r\n                const item = this.thirdItem\r\n                const params = {userId:1,thirdId:id}\r\n                for (let i = 0; i < item.length; i++) {\r\n                    if (item[i].id == id) {\r\n                        if (item[i].isCollect === 1){\r\n                            item[i].isCollect = 0\r\n                        } else {\r\n                            item[i].isCollect = 1\r\n                        }\r\n                    }\r\n                    this.thirdItem = item\r\n                }\r\n                changeCollect(params).then(res=>{\r\n                    if (res.status === 200) {\r\n                        // this.onPullingDown()\r\n                    }\r\n                })\r\n            },\r\n\r\n            handClick(item, id){\r\n                this.data2 = {text: '参数选择'}\r\n                this.data1 = {text: '产品特性'}\r\n                this.pagination.page = 1\r\n                this.thirdItem = []\r\n                this.id = id\r\n                this.searchcontent=\"\"\r\n                this.screen = false\r\n                this.search = false\r\n                const selectedOptions1 = this.selectOptions1\r\n                const selectedOptions2 = this.selectOptions2\r\n                selectedOptions1.style = \"selectUl\"\r\n                selectedOptions2.style = \"selectUl\"\r\n                for (let i = 0; i < selectedOptions1.length; i++) {\r\n                    if (selectedOptions1[i].id == id) {\r\n                        selectedOptions1[i].style = \"selectedUl\"\r\n                        this.selectOptions1 = selectedOptions1\r\n                    }\r\n                }\r\n                for (let i = 0; i < selectedOptions1.length; i++) {\r\n                    if (selectedOptions1[i].id !== id) {\r\n                        selectedOptions1[i].style = \"selectUl\"\r\n                        this.selectOptions1 = selectedOptions1\r\n                    }\r\n                }\r\n                for (let i = 0; i < selectedOptions2.length; i++) {\r\n                    if (selectedOptions2[i].id == id) {\r\n                        selectedOptions2[i].style = \"selectedUl\"\r\n                        this.selectOptions2 = selectedOptions2\r\n                    }\r\n                }\r\n                for (let i = 0; i < selectedOptions2.length; i++) {\r\n                    if (selectedOptions2[i].id !== id) {\r\n                        selectedOptions2[i].style = \"selectUl\"\r\n                        this.selectOptions2 = selectedOptions2\r\n                    }\r\n                }\r\n                if (id===1||id===2) {\r\n                    const parms = {firstId:item.id}\r\n                    getSecondDirectory(parms).then(res=>{\r\n                        if (res.status === 200) {\r\n                            this.secondItem =res.data\r\n                            this.secondItem.forEach( value => {\r\n                                return value.secondImg = BASE_API + value.secondImg\r\n                            })\r\n                        }\r\n                    })\r\n\r\n                    this.secondClass = true\r\n                    this.thirdClass = false\r\n                    this.top = true\r\n                    if (id === 1) {\r\n                        this.screen = true\r\n                    }\r\n                    if (id === 2) {\r\n                        this.screen = false\r\n                        this.top = false\r\n                    }\r\n                }\r\n                if (id === \"search\") {\r\n                    // this.getSearchDirectoryData()\r\n                    this.secondClass = false\r\n                    this.thirdClass = true\r\n                    this.search = true\r\n                    this.top = true\r\n                    this.onPullingDown()\r\n                }\r\n                if (id === \"collect\") {\r\n                    // this.getSearchDirectoryData()\r\n                    this.secondClass = false\r\n                    this.thirdClass = true\r\n\r\n                    this.top = false\r\n                    this.onPullingDown()\r\n                }\r\n\r\n            },\r\n            showSelected() {\r\n              this.show = !this.show\r\n              this.disabled1=false,\r\n              this.disabled2=false\r\n            },\r\n            goNext() {\r\n                this.$router.push({path: '/information'});\r\n                this.disabled1=false;\r\n                this.disabled2=false;\r\n            },\r\n            toggle(index) {\r\n                this.$refs.checkboxes[index].toggle();\r\n            },\r\n            selectAll(){\r\n                if(this.result.length!==this.products.length){\r\n                    for(let i=0;i<this.products.length;i++){\r\n                        this.$refs.checkboxes[i].toggle(true);\r\n                    }\r\n                }\r\n            },\r\n            selectNotAll(){\r\n                if(this.result.length!==0){\r\n                    for(let i=0;i<this.products.length;i++){\r\n                        this.$refs.checkboxes[i].toggle(false);\r\n                    }\r\n      